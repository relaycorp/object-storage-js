{
  "name": "@relaycorp/object-storage",
  "version": "1.0.0",
  "author": {
    "email": "no-reply@relaycorp.tech",
    "name": "Relaycorp, Inc.",
    "url": "https://relaycorp.tech/"
  },
  "description": "Node.js client for object stores like AWS S3, GCP GCS or Minio",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/relaycorp/object-storage-js",
  "homepage": "https://docs.relaycorp.tech/object-storage-js/",
  "license": "MIT",
  "keywords": [
    "gcs",
    "minio",
    "s3"
  ],
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "static-checks": "run-p static-checks:*",
    "static-checks:lint": "tslint --project .",
    "static-checks:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test": "jest --coverage",
    "test:ci:unit": "run-s build test:ci:unit:jest",
    "test:ci:unit:jest": "jest --config jest.config.ci.js --coverage",
    "pretest:integration": "run-s build",
    "test:integration": "src/integration_tests/run.sh",
    "doc-api": "typedoc src/index.ts --out build/docs",
    "clean": "del-cli build test"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.9.5",
    "aws-sdk": "^2.1399.0",
    "verror": "^1.10.1"
  },
  "devDependencies": {
    "@relaycorp/shared-config": "^1.9.4",
    "@types/jest": "^29.5.2",
    "@types/minio": "^7.0.18",
    "@types/verror": "^1.10.6",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.3",
    "env-var": "^7.3.1",
    "gh-pages": "^5.0.0",
    "jest": "^29.5.0",
    "jest-date-mock": "^1.0.8",
    "jest-extended": "^4.0.0",
    "minio": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typedoc": "^0.24.8",
    "typescript": "^5.0.4"
  },
  "prettier": "@relaycorp/shared-config/.prettierrc.json",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
